{
  "schemaVersion": 1,
  "id": "shenanigans",
  "version": "${version}",

  "name": "Shenanigans",
  "description": "Mod by Myoun",
  "authors": [
    "myoun"
  ],
  "contact": {
    "sources": "https://github.com/myoun/shenanigans"
  },

  "license": "MIT",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "app.myoun.shenanigans.ShenanigansMod"
      }
    ]
  },
  "mixins": [
    "shenanigans.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": "*",
    "minecraft": "~${minecraft_version}",
    "java": ">=17",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}"
  },
  "suggests": {
    "another-mod": "*"
  }
}
